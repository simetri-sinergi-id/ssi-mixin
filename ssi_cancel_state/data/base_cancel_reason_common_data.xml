<odoo>
    <data noupdate="1">

        <record id="base_cancel_reason_01" model="base.cancel.reason">
            <field name="name">Data Entry Mistakes</field>
            <field name="default">1</field>
            <field name="type">fixed</field>
            <field
                name="description"
            >This data was cancelled because user has a mistake when entering data</field>
        </record>

        <record id="base_cancel_reason_02" model="base.cancel.reason">
            <field name="name">Duplicate Data</field>
            <field name="default">1</field>
            <field name="type">not_fixed</field>
            <field
                name="description"
            >This data was cancelled because it was a duplicate</field>
        </record>

        <record id="base_cancel_reason_03" model="base.cancel.reason">
            <field name="name">Deadline</field>
            <field name="default">1</field>
            <field name="type">fixed</field>
            <field
                name="description"
            >This data was cancelled because it was unable to meet the deadline</field>
        </record>

        <record id="base_cancel_reason_04" model="base.cancel.reason">
            <field name="name">Other</field>
            <field name="default">1</field>
            <field name="type">not_fixed</field>
            <field
                name="description"
            >This data was cancelled because user has other reason</field>
        </record>
    </data>
</odoo>
